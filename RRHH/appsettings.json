{
  "AppSettings": {
    "Secret": "<YOUT JWT SECRET KEY>",
    "ValidMinutes": "60"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "local": "<LOCAL DB CONNECTION STRING>"
  },
  "emailconfig": {
    "senderemail": "<YOUT SENDER EMAIL>",
    "password": "<YOUT SENDER EMAIL PASSWORD>",
    "port": "<SMTP PORT>",
    "host": "<SMTP HOST ADDRESS>",
    "activationURL": "http://localhost:4200/invitado/usuariocreado",
    "activationURLProd": "<YOUR REMOTE WEBSITE ADDRESS>/invitado/usuariocreado",
    "restorePasswordURL": "http://localhost:4200/invitado/cambiarcontrasena",
    "restorePasswordURLProd": "<YOUR REMOTE WEBSITE ADDRESS>/invitado/cambiarcontrasena"
  }
}
